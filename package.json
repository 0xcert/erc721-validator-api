{
  "name": "erc721ValidatorApi",
  "description": "API to use ERC-721 validator",
  "version": "0.1.0",
  "license": "Private",
  "main": "./dist/index.js",
  "types": "./lib/index.d.ts",
  "author": "Leon Panjtar <leon@kalmia.si>",
  "scripts": {
    "development:build-dist": "./bin/development/build-dist.sh",
    "development:clean-dist": "./bin/development/clean-dist.sh",
    "development:lint": "./bin/development/lint.sh",
    "development:print-env": "./bin/development/print-env.sh",
    "development:print-routes": "./bin/development/print-routes.sh",
    "development:run": "node -r ts-node/register --inspect ./src/scripts/start-http",
    "development:start-http": "./bin/development/start-http.sh",
    "production:print-env": "./bin/production/print-env.sh",
    "production:print-routes": "./bin/production/print-routes.sh",
    "production:start-http": "./bin/production/start-http.sh",
    "test:print-env": "./bin/test/print-env.sh",
    "test:print-routes": "./bin/test/print-routes.sh",
    "test:run-tests": "./bin/test/run-tests.sh",
    "dev": "./bin/dev.sh",
    "start": "./bin/start.sh",
    "test": "./bin/test.sh",
    "create": "claudia create --profile 0xcert --source dist --handler lambda.handler --role 0xcert-api-deployment-executor --use-s3-bucket 0xcert-lambda-uploads --use-local-dependencies --memory 1024 --timeout 300 --deploy-proxy-api --region eu-central-1 --set-env-from-json bin/env/env.json --config bin/env/claudia-prod.json --name erc721validator-api",
    "update": "cp -R node_modules dist/node_modules && claudia update --source dist --handler lambda.handler --role 0xcert-api-deployment-executor --use-s3-bucket 0xcert-lambda-uploads --use-local-dependencies --memory 1024 --timeout 300 --deploy-proxy-api --region eu-central-1 --config bin/env/claudia-prod.json --name erc721validator-api",
    "production:build": "docker run --rm -v $PWD:/app 0423d0927cbf"
  },
  "engines": {
    "node": ">= 9"
  },
  "dependencies": {
    "@0xcert/erc721-validator": "^0.1.4",
    "aws-serverless-express": "^3.3.5",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "cli-table2": "^0.2.0",
    "cors": "^2.8.4",
    "express": "^5.0.0-alpha.6",
    "rawmodel": "^1.32.0",
    "scrypt": "^6.0.3",
    "web3": "1.0.0-beta.34"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.1",
    "@types/body-parser": "^1.16.8",
    "@types/cli-table2": "^0.2.1",
    "@types/cors": "^2.8.3",
    "@types/express": "^4.11.1",
    "@types/faker": "^4.1.2",
    "@types/jsonwebtoken": "^7.2.6",
    "@types/mocha": "^5.0.0",
    "@types/node": "^9.6.5",
    "@types/supertest": "^2.0.4",
    "mocha": "^5.1.0",
    "supertest": "^3.0.0",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.8.1",
    "web3-typescript-typings": "^0.10.2"
  }
}
